{
    "name": "KKeyps",
    "description": "Nvm",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage", 
        "declarativeContent", 
        "activeTab", 
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://kubaboi.github.io/;"
    }
}