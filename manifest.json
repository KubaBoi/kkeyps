{
    "name": "KKeyps",
    "description": "Nvm",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage", 
        "declarativeContent", 
        "activeTab", 
        "scripting",
        "contextMenus",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://*/"
    ]
}